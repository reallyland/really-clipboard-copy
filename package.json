{
  "name": "really-clipboard-copy",
  "version": "0.0.0",
  "private": true,
  "description": "Really custom element to copy content to clipboard",
  "keywords": [
    "really-clipboard-copy",
    "lit-element",
    "lit-html",
    "polymer",
    "typescript",
    "web-components"
  ],
  "homepage": "https://github.com/reallyland/really-clipboard-copy",
  "repository": {
    "type": "git",
    "url": "git@github.com:reallyland/really-clipboard-copy.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng (motss)",
    "email": "wes.ngrongsen@gmail.com",
