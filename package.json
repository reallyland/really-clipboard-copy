  },
  "files": [
    "dist/*.d.ts.map",
    "dist/*.d.ts",
    "dist/*.js.map",
    "dist/*.js"
  ],
  "main": "./dist/really-clipboard-copy.js",
  "module": "./dist/really-clipboard-copy.js",
  "typings": "./dist/really-clipboard-copy.d.ts",
  "scripts": {
    "build": "npm run ts -- -p tsconfig.prod.json",
    "clean": "rm -rf dist/",
