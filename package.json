    "rollup-plugin-typescript2": "^0.20.1",
    "tslint": "^5.15.0",
    "typescript": "^3.4.2",
    "wct-mocha": "^1.0.0"
  },
  "engines": {
    "node": ">= 10.15.1",
    "npm": ">= 6.4.1"
  },
  "publishConfig": {
    "access": "public"
