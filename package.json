  "dependencies": {
    "lit-element": "^2.1.0",
    "lit-html": "^1.0.0"
  },
  "devDependencies": {
    "@messageflow/tslint-config": "^1.3.0",
    "@reallyland/tsconfig": "^1.0.0",
    "@types/chai": "^4.1.7",
